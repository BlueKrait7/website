      "code": "<span class=\"hljs-keyword\">using</span> System;\r\n<span class=\"hljs-keyword\">using</span> System.Collections.Generic;\r\n<span class=\"hljs-keyword\">using</span> System.Numerics;\r\n\r\n<span class=\"hljs-keyword\">namespace</span> <span class=\"hljs-title\">Algorithms.Sequences</span>\r\n{\r\n    <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span> <span class=\"hljs-doctag\">&lt;summary&gt;</span></span>\r\n    <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>     <span class=\"hljs-doctag\">&lt;para&gt;</span></span>\r\n    <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>         Number of ways of making change for n cents using coins of 1, 2, 5, 10 cents.</span>\r\n    <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>     <span class=\"hljs-doctag\">&lt;/para&gt;</span></span>\r\n    <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>     <span class=\"hljs-doctag\">&lt;para&gt;</span></span>\r\n    <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>         OEIS: https://oeis.org/A000008.</span>\r\n    <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>     <span class=\"hljs-doctag\">&lt;/para&gt;</span></span>\r\n    <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span> <span class=\"hljs-doctag\">&lt;/summary&gt;</span></span>\r\n    <span class=\"hljs-keyword\">public</span> <span class=\"hljs-keyword\">class</span> <span class=\"hljs-title\">MakeChangeSequence</span> : <span class=\"hljs-title\">ISequence</span>\r\n    {\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span> <span class=\"hljs-doctag\">&lt;summary&gt;</span></span>\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>     <span class=\"hljs-doctag\">&lt;para&gt;</span></span>\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>         Gets sequence of number of ways of making change for n cents</span>\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>         using coins of 1, 2, 5, 10 cents.</span>\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>     <span class=\"hljs-doctag\">&lt;/para&gt;</span></span>\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>     <span class=\"hljs-doctag\">&lt;para&gt;</span></span>\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>         Uses formula from OEIS page by Michael Somos</span>\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>         along with first 17 values to prevent index issues.</span>\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>     <span class=\"hljs-doctag\">&lt;/para&gt;</span></span>\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>     <span class=\"hljs-doctag\">&lt;para&gt;</span></span>\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>         Formula:</span>\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>         a(n) = a(n-2) +a(n-5) - a(n-7) + a(n-10) - a(n-12) - a(n-15) + a(n-17) + 1.</span>\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span>     <span class=\"hljs-doctag\">&lt;/para&gt;</span></span>\r\n        <span class=\"hljs-comment\"><span class=\"hljs-doctag\">///</span> <span class=\"hljs-doctag\">&lt;/summary&gt;</span></span>\r\n        <span class=\"hljs-keyword\">public</span> IEnumerable&lt;BigInteger&gt; Sequence\r\n        {\r\n            <span class=\"hljs-keyword\">get</span>\r\n            {\r\n                <span class=\"hljs-keyword\">var</span> seed = <span class=\"hljs-keyword\">new</span> List&lt;BigInteger&gt;\r\n                           {\r\n                               <span class=\"hljs-number\">1</span>, <span class=\"hljs-number\">1</span>, <span class=\"hljs-number\">2</span>, <span class=\"hljs-number\">2</span>, <span class=\"hljs-number\">3</span>, <span class=\"hljs-number\">4</span>, <span class=\"hljs-number\">5</span>, <span class=\"hljs-number\">6</span>, <span class=\"hljs-number\">7</span>, <span class=\"hljs-number\">8</span>,\r\n                               <span class=\"hljs-number\">11</span>, <span class=\"hljs-number\">12</span>, <span class=\"hljs-number\">15</span>, <span class=\"hljs-number\">16</span>, <span class=\"hljs-number\">19</span>, <span class=\"hljs-number\">22</span>, <span class=\"hljs-number\">25</span>,\r\n                           };\r\n                <span class=\"hljs-keyword\">foreach</span> (<span class=\"hljs-keyword\">var</span> <span class=\"hljs-keyword\">value</span> <span class=\"hljs-keyword\">in</span> seed)\r\n                {\r\n                    <span class=\"hljs-keyword\">yield</span> <span class=\"hljs-keyword\">return</span> <span class=\"hljs-keyword\">value</span>;\r\n                }\r\n\r\n                <span class=\"hljs-keyword\">for</span>(<span class=\"hljs-keyword\">var</span> index = <span class=\"hljs-number\">17</span>; ; index++)\r\n                {\r\n                    BigInteger newValue = seed[index - <span class=\"hljs-number\">2</span>] + seed[index - <span class=\"hljs-number\">5</span>] - seed[index - <span class=\"hljs-number\">7</span>]\r\n                                        + seed[index - <span class=\"hljs-number\">10</span>] - seed[index - <span class=\"hljs-number\">12</span>] - seed[index - <span class=\"hljs-number\">15</span>]\r\n                                        + seed[index - <span class=\"hljs-number\">17</span>] + <span class=\"hljs-number\">1</span>;\r\n\r\n                    seed.Add(newValue);\r\n                    <span class=\"hljs-keyword\">yield</span> <span class=\"hljs-keyword\">return</span> newValue;\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n"
    }
